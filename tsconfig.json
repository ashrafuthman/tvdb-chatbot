{
  "compilerOptions": {
    "target": "ES2022",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "lib": ["ES2022"],
    "types": ["node"],
    "skipLibCheck": true,
    "noEmit": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}
